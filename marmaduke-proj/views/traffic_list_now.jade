extends root_layout

block content
    table
        thead
            th="TRIP"
            th="SHIP"
            th="CONTAINER"
            th="CAMION"
            th="PASSENGERS"
            th="TYPE"
        each cuData in allDataNow
            tr
                td=cuData.id_control_unit_data
                td=cuData.ship_description
                td=cuData.num_container
                td=cuData.num_camion
                td=cuData.num_passengers
                td=cuData.traffic_list_mvnt_type
                