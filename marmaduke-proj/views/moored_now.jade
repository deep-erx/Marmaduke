extends layout

block content
    table
        thead
            th="TRIP"
            th="SHIP"
            th="STATE"
            th="TIMESTAMP"
        each cuData in allTripsActive
            tr
                td=cuData.id_control_unit_data
                td=cuData.ship_description
                td=cuData.state_name
                td=cuData.ts_main_event_field_val